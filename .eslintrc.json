{
  "plugins": ["prettier"],
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "no-undef": "error",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "quotes": ["error", "single"],
    "no-console": "error",
    "@typescript-eslint/no-explicit-any": 0,
    "no-use-before-define": "off",
    "no-unused-vars": "error"
  }
}
